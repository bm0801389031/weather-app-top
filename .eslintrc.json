{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "env": {
      "browser": true
    },
    "rules": {
      "no-alert": "off",
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "all",
          "semi": true,
          "printWidth": 80,
          "tabWidth": 4,
          "endOfLine": "auto"
        }
      ]
    }
  }
  